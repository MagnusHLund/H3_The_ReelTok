{
  "expo": {
    "plugin": [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let you upload videos."
      }
    ],
    "name": "the_reeltok_client",
    "slug": "the_reeltok_client",
    "version": "1.0.0",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.the_reeltok_client"
    },
    "web": {},
    "plugins": [
      "expo-video",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ]
    ]
  }
}
